#!/usr/bin/env ruby
require 'awesome_parser'

require 'open-uri'
require 'awesome_print'

# List of Lists
# https://raw.githubusercontent.com/sindresorhus/awesome/master/readme.md

text = open(ARGV[0]||"https://raw.githubusercontent.com/markets/awesome-ruby/master/README.md").read
awesome_parser = AwesomeParser::Parser.new(text)
awesome_parser.parse!

ap awesome_parser.items
