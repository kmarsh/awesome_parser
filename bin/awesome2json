#!/usr/bin/env ruby
require 'awesome_parser'
require 'json'

require 'awesome_print'

# List of Lists
# https://raw.githubusercontent.com/sindresorhus/awesome/master/readme.md

awesome_parser = AwesomeParser::Parser.new(ARGF.read)
awesome_parser.parse!

awesome_parser.items.each do |item|
  item.delete(:line)

  puts item.to_json
end
